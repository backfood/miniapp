.page {
  padding: 0 30rpx 0 30rpx;
}
.header {
  margin: 27rpx 0 29rpx 0;
}
.header .item {
  float: left;
  color: #ffffff;
  background-color: #b0ebde;
  margin-right: 30rpx;
  border-radius: 4rpx;
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
}
.header .active {
  background-color: #44D7B6;
}
.clearfix:after {
  content: "";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
